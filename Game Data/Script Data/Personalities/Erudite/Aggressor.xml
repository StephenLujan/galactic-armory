<?xml version="1.0"?>
<personality>
	<!-- Goes to war more often -->
	<var id="ConsiderWarStrengthRatio" value="1.5"/>
	<var id="ConsiderBribeStrengthRatio" value="0.3"/>
	<var id="ConsiderObliterateStrengthRatio" value="2.5"/>
	<!-- Gets bored more quickly -->
	<var id="BoredomFactor" value="1.08"/>
	<var id="BoredomNoBribes" value="4"/>
	<!-- Will attack with fleets more quickly -->
	<var id="FleetAttackRatio" value="1.5"/>
	<var id="FleetRetreatRatio" value="0.3"/>
	<!-- Determines how much ships will be upscaled per level -->
	<var id="ScalingCurve" value="1.3"/>
	<!-- Race Description -->
	<race name="Aggressor" desc="Coming from a clan-based society, personal gain is less important than increasing the power and glory of one's own clan. These social structures even endured the beginning conquest of space, making expansion at any cost a race between different clans rather than different species.
	
	This species utilizies a mix of fast, hard-hitting and heavily armored ships; often in large numbers due to their economic design."/>
	<!-- More technologies to research and unlock -->
	<list id="ResearchTechs" mode="append">
		<li>BeamWeapons</li>
		<li>Missiles</li>
		<li>Shields</li>
		<li>Computers</li>
		<li>WarpPhysics</li>
		<li>Sensors</li>
	</list>
	<list id="LinkTechs">
		<li>Armor</li>
		<li>Missiles</li>
		<li>MegaConstruction</li>
		<li>Chemistry</li>
		<li>Biology</li>
		<li>Computers</li>
		<li>Sociology</li>
		<li>BeamWeapons</li>
		<li>Sensors</li>
		<li>WarpPhysics</li>
		<li>Shields</li>
	</list>
	<list id="LinkFromTechs">
		<li>ProjWeapons</li>
		<li>ProjWeapons</li>
		<li>ShipConstruction</li>
		<li>Science</li>
		<li>Science</li>
		<li>Biology</li>
		<li>Biology</li>
		<li>EnergyPhysics</li>
		<li>EnergyPhysics</li>
		<li>ShipSystems</li>
		<li>EnergyPhysics</li>
	</list>
	<list id="Traits">
		<li>economical</li>
		<li>bustling</li>
		<li>colonialism</li>
		<li>natural_research</li>
		<li>goodbiology</li>
		<li>randtech</li>
		<li>developed</li>		
	</list>
	<!-- Layouts -->
	<layouts>
		<!-- Fuel Tanker Line -->
		<default name="Fuel Tanker" file="Layouts/AggressorErudite/Fuel Tanker" goal="Tanker" autoscale="false"/>
		<!-- Railgun Cruiser Line -->
		<default name="Cancer-class Cruiser" file="Layouts/AggressorErudite/Cancer-class Cruiser" goal="leader"/>
		<design name="Cancer II-class Cruiser" file="Layouts/AggressorErudite/Cancer II-class Cruiser" goal="Leader" replaces="Cancer-class Cruiser"/>
		<design name="Cancer III-class Cruiser" file="Layouts/AggressorErudite/Cancer III-class Cruiser" goal="Leader" replaces="Cancer II-class Cruiser"/>
		<design name="Cancer IV-class Cruiser" file="Layouts/AggressorErudite/Cancer IV-class Cruiser" goal="Leader" replaces="Cancer III-class Cruiser"/>
		<!-- Porcupine Cruiser Line -->
		<default name="Hammerhead-class Frigate" file="Layouts/AggressorErudite/Hammerhead-class Frigate" goal="leader"/>
		<design name="Hammerhead II-class Frigate" file="Layouts/AggressorErudite/Hammerhead II-class Frigate" goal="Leader" replaces="Hammerhead-class Frigate"/>
		<design name="Hammerhead III-class Frigate" file="Layouts/AggressorErudite/Hammerhead III-class Frigate" goal="Leader" replaces="Hammerhead II-class Frigate"/>
		<design name="Hammerhead IV-class Frigate" file="Layouts/AggressorErudite/Hammerhead IV-class Frigate" goal="Leader" replaces="Hammerhead III-class Frigate"/>
