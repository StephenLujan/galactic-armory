		<design name="Hydra III-class Cruiser" file="Layouts/AggressorErudite/Hydra III-class Cruiser" goal="CombatSpecialized" replaces="Hydra II-class Cruiser"/>
		<design name="Hydra IV-class Cruiser" file="Layouts/AggressorErudite/Hydra IV-class Cruiser" goal="CombatSpecialized" replaces="Hydra III-class Cruiser"/>
		<!-- Railgun Frigate Line -->
		<default name="Warhound-class Frigate" file="Layouts/AggressorErudite/Warhound-class Frigate" goal="Leader"/>
