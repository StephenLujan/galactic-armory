		<design name="Vengance-class Battlecruiser" file="Layouts/AggressorErudite/Vengeance-class Battlecruiser" goal="Combat" minTime="35"/>
		<design name="Vengance II-class Battlecruiser" file="Layouts/AggressorErudite/Vengeance II-class Battlecruiser" goal="Combat" replaces="Vengance-class Battlecruiser"/>
		<design name="Vengance III-class Battlecruiser" file="Layouts/AggressorErudite/Vengeance III-class Battlecruiser" goal="Combat" replaces="Vengance II-class Battlecruiser" scalefrom="16"/>
		<design name="Vengance IV-class Battlecruiser" file="Layouts/AggressorErudite/Vengeance IV-class Battlecruiser" goal="Combat" replaces="Vengance III-class Battlecruiser" scalefrom="18"/>
		<design name="Vengance V-class Battlecruiser" file="Layouts/AggressorErudite/Vengeance V-class Battlecruiser" goal="Combat" replaces="Vengance IV-class Battlecruiser" scalefrom="20"/>
		<!-- Interceptor Line -->
		<default name="Light Fighter" file="Layouts/AggressorErudite/Light Fighter" goal="Fighter" autoscale="false"/>
		<design name="Light Interceptor" file="Layouts/AggressorErudite/Light Interceptor" goal="Fighter" replaces="Light Fighter" autoscale="false"/>
